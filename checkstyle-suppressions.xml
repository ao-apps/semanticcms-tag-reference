<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- TODO: Complete Javadocs -->
  <suppress checks="(MissingJavadocMethod|MissingJavadocType|SummaryJavadoc)" />

  <!-- Will keep "EE" suffix for consistency with "Java EE" and "Jakarta EE" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]semanticcms[/\\]tagreference[/\\]TagReferenceInitializer\.java$"
    message="'javadocLinkJavaEE'"
  />

  <!-- Will keep "SE" suffix for consistency with "Java SE" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]semanticcms[/\\]tagreference[/\\]TagReferenceInitializer\.java$"
    message="'javadocLinkJavaSE'"
  />

</suppressions>
